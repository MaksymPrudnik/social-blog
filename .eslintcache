bscription && !subscription))) {\n                try {\n                    const registration = await navigator.serviceWorker.ready;\n                    let newSubscription = await registration.pushManager.getSubscription();\n                    \n                    if (!newSubscription) {\n                        newSubscription = await registration.pushManager.subscribe({\n                            userVisibleOnly: true,\n                            applicationServerKey: urlBase64ToUint8Array(pushServerPublicKey)\n                        })\n                    }\n                    \n                    if (newSubscription) return setUserSubscription(newSubscription);\n\n                    throw new Error('Subscription object wasn\\'t found and programm failed to create new one')\n                } catch (error) {\n                    setError(error)\n                }\n            }\n        }\n        checkSubscription();\n    }, [subscription, userSubscription])\n\n    useEffect(() => {\n        if (!subscription && Notification.permission === 'granted' && userSubscription) {\n            const token = localStorage.getItem('token');\n            sendSubscriptionToServer(token);\n        }\n    }, [subscription, userSubscription])\n\n    const sendSubscriptionToServer = (token) => {\n        setLoading(true);\n        fetch(`${host}/add-subscription`, {\n            method: 'post',\n            headers: {\n                'Content-Type': 'application/json',\n                'Authorization': token\n            },\n            body: JSON.stringify({\n                subscription: userSubscription\n            })\n        })\n        .then(response => {\n            if (response.status === 202) {\n                dispatch(subscriptionAddedAction(userSubscription))\n            }\n            return Promise.reject('Error sending subscription to server')\n        })\n        .catch(err => setError(err))\n        .finally(() => setLoading(false))\n    }\n\n    const subscribeToNotifications = (token) => {\n        if (Notification.permission !== 'granted') {\n            Notification.requestPermission(consent => {\n                if (consent === 'granted') {\n                    sendSubscriptionToServer(token)\n                }\n            })\n        }\n    }\n\n    return {\n        loading,\n        error,\n        subscribeToNotifications\n    }\n\n}\n\nexport default usePushNotification;","/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/components/Friends/FriendBox/FriendBox.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/components/Posts/Comments/Comment.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/components/Routes/Home/Home.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/getUserAction.js",[],["362","363"],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/authActions.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/getPostListAction.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/getProfileAction.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/clearProfileAction.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/updateUserActions.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/deletePostAction.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/updatePostAction.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/state/actions/friendsAction.js",[],"/home/maksymprudnik/Desktop/dev/narnia/personal-projects/sociaten/social-blog/src/sta